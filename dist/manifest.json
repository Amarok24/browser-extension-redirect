{
	"name": "Preview Redirect",
	"description": "Monster Preview link redirection",
	"author": "Jan Prazak",
	"homepage_url": "https://github.com/Amarok24",
	"version": "1.0.2",
	"manifest_version": 3,
	"permissions": [
		"declarativeNetRequest"
	],
	"host_permissions": [
		"*://draft.monster.co.uk/*"
	],
	"declarative_net_request": {
		"rule_resources": [
			{
				"id": "ruleset_1",
				"enabled": true,
				"path": "rules.json"
			}
		]
	}
}
